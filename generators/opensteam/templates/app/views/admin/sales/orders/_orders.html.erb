<% grid_table do %>
  <thead>
    <tr>
      <th class="first no-menu no-sort not-in-context no-resize"><div>&nbsp;</div></th>
      <th onclick="clickCatcher(this);"><div>ID</div></th>
      <th class="active"><div>Items</div></th>
      <th><div>Customer</div></th>
      <th><div>Shipping Address</div></th>
      <th><div>Payment Address</div></th>
      <th><div>State</div></th>
      <th><div>Created At</div></th>
      <th><div>Updated At</div></th>
      <th class="last not-in-context"><div>&nbsp;</div></th>
    </tr>
  </thead>
  <tbody id="orders_table_body">
    <%= render :partial => "admin/sales/orders/order", :collection => orders %>
  </tbody>
<% end %>
